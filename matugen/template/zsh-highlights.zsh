# zsh-syntax-highlighting colors (matugen)
ZSH_HIGHLIGHT_STYLES[command]='fg={{colors.primary.default.hex}},bold'
ZSH_HIGHLIGHT_STYLES[builtin]='fg={{colors.primary.default.hex}},bold'
ZSH_HIGHLIGHT_STYLES[alias]='fg={{colors.primary.default.hex}},bold'
ZSH_HIGHLIGHT_STYLES[unknown-token]='fg={{colors.error.default.hex}},bold'
ZSH_HIGHLIGHT_STYLES[path]='fg={{colors.tertiary.default.hex}}'
ZSH_HIGHLIGHT_STYLES[string]='fg={{colors.secondary.default.hex}}'
ZSH_HIGHLIGHT_STYLES[comment]='fg={{colors.outline.default.hex}}'
